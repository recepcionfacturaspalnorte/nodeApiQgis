generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model poligonos_productores {
  gid              Int                      @id @default(autoincrement())
  fid              Decimal?                 @db.Decimal
  id               Float?
  CODIGO_PROVEEDOR String?                  @db.VarChar(254)
  PROVEEDOR        String?                  @db.VarChar(50)
  TIPO_PROVEEDOR   String?                  @db.VarChar(254)
  CLASIFICACION    String?                  @db.VarChar(254)
  FINCA            String?                  @db.VarChar(50)
  CODIGO_FINCA     String?                  @db.VarChar(254)
  LOTE             String?                  @db.VarChar(50)
  AREA_ha_         Decimal?                 @map("AREA(ha)") @db.Decimal
  VEREDA           String?                  @db.VarChar(50)
  ZONA             String?                  @db.VarChar(50)
  municipio        String?                  @db.VarChar(20)
  material         String?                  @db.VarChar(30)
  etapa            String?                  @db.VarChar(50)
  NUMERO_PALMAS    Decimal?                 @db.Decimal
  DISTANCIA_PALMAS Decimal?                 @db.Decimal
  cobertura        String?                  @db.VarChar(80)
  FECHA_DE_SIEMBRA DateTime?                @map("FECHA DE SIEMBRA") @db.Date
  edad_palma       Float?
  tipo             String?                  @db.VarChar(80)
  uma              Float?
  asociacion       String?                  @db.VarChar(200)
  observacio       String?                  @db.VarChar(254)
  WKT              String?                  @db.Text
  CODIGO_LOTE      String?                  @db.VarChar(254)
  geom             Unsupported("geometry")?

  @@index([geom], type: Gist)
}

